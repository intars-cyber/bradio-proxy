{
  "version": 2,
  "functions": {
    "api/stream.js": {
      "memory": 128,
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/api/stream",
      "dest": "/api/stream.js"
    },
    {
      "src": "/(.*)",
      "status": 302,
      "headers": {
        "Location": "https://bradio.dev"
      }
    }
  ]
}